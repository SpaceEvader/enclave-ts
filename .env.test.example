# Copy this file to .env.test and fill in your test credentials
# WARNING: Never commit .env.test with actual credentials!

# Enclave Markets API Test Credentials
ENCLAVE_TEST_API_KEY=your_test_api_key_here
ENCLAVE_TEST_API_SECRET=your_test_api_secret_here

# Test environment (SANDBOX or SANDBOX_PERMISSIONLESS recommended)
ENCLAVE_TEST_ENVIRONMENT=SANDBOX_PERMISSIONLESS

# Test market to use for order tests (should have liquidity)
ENCLAVE_TEST_MARKET=BTC-USD.P

# Enable debug logging for tests
ENCLAVE_TEST_DEBUG=false
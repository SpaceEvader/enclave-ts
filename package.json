{
  "name": "@enclave-markets/client",
  "version": "0.1.0",
  "description": "TypeScript client library for Enclave Markets API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "check": "npm run build && npm run lint && npm run typecheck && npm run test",
    "docs:generate": "typedoc --out docs/api src/index.ts",
    "docs:serve": "npx serve docs",
    "prepublishOnly": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpaceEvader/enclave-ts.git"
  },
  "keywords": [
    "enclave",
    "markets",
    "trading",
    "api",
    "client",
    "typescript",
    "perpetuals",
    "crypto"
  ],
  "author": "SpaceEvader",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SpaceEvader/enclave-ts/issues"
  },
  "homepage": "https://github.com/SpaceEvader/enclave-ts#readme",
  "dependencies": {
    "decimal.js": "^10.4.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}